    "colors": "^1.4.0",
    "config": "^3.3.1",
    "cookie-parser": "^1.4.5",
    "cross-env": "^7.0.2",
    "express": "^4.17.1",
    "formidable": "^1.2.2",
    "http-errors": "^1.8.0",
    "jsonwebtoken": "^8.5.1",
    "log4js": "^6.3.0",
    "md5-node": "^1.0.1",
    "mysql": "^2.18.1",
    "pm2": "^4.5.5",
    "require-dir": "^1.2.0",
    "validator-xingsk": "^1.0.5"
  }
